{
  "expo": {
    "name": "QrTec",
    "slug": "QrTec",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/qr_code.png",
    "scheme": "qrtec",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/qr_code.png",
        "backgroundImage": "./assets/qr_code.png",
        "monochromeImage": "./assets/qr_code.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jide73.QrTec"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/qr_code.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/qr_code.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e489fcf2-31af-489d-8d11-d43555ca4030"
      }
    }
    
  }
}
